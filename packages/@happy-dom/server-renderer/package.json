{
	"name": "@happy-dom/server-renderer",
	"version": "0.0.0",
	"license": "MIT",
	"homepage": "https://github.com/capricorn86/happy-dom/tree/master/packages/@happy-dom/server-renderer",
	"repository": "https://github.com/capricorn86/happy-dom",
	"author": "David Ortner",
	"description": "Use Happy DOM for server-side rendering (SSR) or as a static site generator (SSG).",
	"main": "lib/index.js",
	"type": "module",
	"keywords": [
		"jsdom",
		"happy",
		"dom",
		"webcomponents",
		"web",
		"component",
		"custom",
		"elements",
		"server-side",
		"rendering",
		"server",
		"renderer",
		"ssr",
		"static",
		"site",
		"generator",
		"ssg"
	],
	"publishConfig": {
		"access": "public"
	},
	"bin": {
		"happy-dom-sr": "bin/lib/happy-dom-sr.js"
	},
	"scripts": {
		"compile": "tsc && tsc --project ./bin",
		"watch": "concurrently --raw \"tsc -w --preserveWatchOutput\" \"tsc -w --preserveWatchOutput --project ./bin\"",
		"test": "vitest run",
		"test:ui": "vitest --ui",
		"test:watch": "vitest",
		"test:debug": "vitest run --inspect-brk --no-file-parallelism"
	},
	"dependencies": {
		"happy-dom": "^0.0.0",
		"chalk": "^5.4.1"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.16.0",
		"@typescript-eslint/parser": "^5.16.0",
		"@vitest/ui": "^2.1.4",
		"@types/node": "^20.0.0",
		"concurrently": "^9.1.2",
		"prettier": "^2.6.0",
		"typescript": "^5.0.4",
		"vitest": "^2.1.4"
	},
	"engines": {
		"node": ">=20.0.0"
	}
}
