{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"compile": {
			"dependsOn": ["^compile"],
			"outputs": ["lib/**"]
		},
		"global-registrator#compile": {
			"dependsOn": ["happy-dom#compile"]
		},
		"lint": {
			"outputs": []
		},
		"lint:fix": {
			"outputs": []
		},
		"test": {
			"outputs": []
		},
		"test:watch": {
			"outputs": []
		},
		"version": {
			"outputs": []
		},
		"publish": {
			"outputs": []
		}
	}
}
